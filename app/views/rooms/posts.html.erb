<div class="container-fluid room-posts">
  <h4 class="pb-4">登録済みルーム一覧</h4>
  <table>
    <thead>
      <tr align="center"> 
        <th>ルーム画像</th>
        <th>ルーム名</th>
        <th>ルーム紹介</th>
        <th>料金</th>
        <th>作成日</th>
      </tr>
    </thead>
    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td><%= room.image %></td>
          <td class="posts-name">
            <%= link_to "#{room.name}", "/rooms/#{room.id}" %>
          </td>
          <td><%= %></td>
          <td><%= %></td>
          <td><%= %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>